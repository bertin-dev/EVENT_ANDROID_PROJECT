<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.event.mobile">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_official"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_official"
        android:supportsRtl="true"
        android:theme="@style/Theme.EVENT_ANDROID_PROJECT">
        <activity android:name=".ui.Profile"></activity>
        <activity android:name=".ui.Register" />
        <activity android:name=".MainActivity" />
        <activity android:name=".ui.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>